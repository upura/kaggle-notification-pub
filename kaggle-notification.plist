<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
 <key>Label</key>
   <string>kernel-notify</string>
  <!--フルパスで指定-->
 <key>ProgramArguments</key>
   <array>
     <string>/usr/local/bin/zsh</string>
     <string>/Users/{user_name}/{project_name}/kernel-notify.sh</string>
   </array>
 <key>StartInterval</key>
   <integer>30</integer>
 <key>RunAtLoad</key>
   <true/>
 <key>StandardOutPath</key>
    <string>/Users/{user_name}/{project_name}/test.out</string>
 <key>StandardErrorPath</key>
    <string>/Users/{user_name}/{project_name}/test.err</string>
</dict>
</plist>
